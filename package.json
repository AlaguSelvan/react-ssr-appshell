{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "private": true,
  "description": "react app with ssr",
  "main": "index.js",
  "author": "Alagu Selvan",
  "license": "MIT",
  "engines": {
    "node": ">=10.7.0",
    "yarn": "^1.13"
  },
  "scripts": {
    "---- // START DEVELOPMENT // ----": "",
    "start": "npm run build:setup && npm run build:dev && npm run start:dev",
    "---- // START PRODUCTION // ----": "",
    "start:prod": "npm run build:setup && npm run build:compile && npm run build:prod && npm run start:live",
    "----- // POST BUILD // -----": "",
    "start:dev": "NODE_ENV=development nodemon src/index.js",
    "start:live": "NODE_ENV=production nodemon build/index.js",
    "----- // BUILD SCRIPTS // -----": "",
    "build:setup": "npm run clean:dist && npm run build:dist",
    "build:compile": "babel src -d build",
    "build:dist": "mkdir -p ./build",
    "build:prod": "set NODE_ENV=production && webpack --mode production --config config/webpack/webpack.client.config.js --progress",
    "build:dev": "set NODE_ENV=development && webpack --mode development --config config/webpack/webpack.client.config.js --progress",
    "----- // STAT SCRIPTS // -----": "",
    "stats": "webpack --profile --json > webpack-stats.json",
    "----- // CLEAN SCRIPTS // -----": "",
    "clean": "npm run clean:dist && npm run clean:public",
    "clean:dist": "rimraf build",
    "clean:public": "rimraf build/public",
    "----- // TEST SCRIPTS // -----": "",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "classnames": "^2.2.6",
