    "react-jss": "^8.6.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-useragent": "^1.1.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "why-did-you-update": "^1.0.6"
  },
